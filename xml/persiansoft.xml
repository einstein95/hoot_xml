			<rom type="file" offset="0x10">SM01.GMD</rom>
			<rom type="file" offset="0x11">SM02.GMD</rom>
			<rom type="file" offset="0x12">SM03.GMD</rom>
			<rom type="file" offset="0x13">SM04.GMD</rom>
			<rom type="file" offset="0x14">SM05.GMD</rom>
			<rom type="file" offset="0x15">SM06.GMD</rom>
			<rom type="file" offset="0x16">SM07.GMD</rom>
			<rom type="file" offset="0x17">SM08.GMD</rom>
			<rom type="file" offset="0x18">SM09.GMD</rom>
			<rom type="file" offset="0x19">SM10.GMD</rom>
			<rom type="file" offset="0x1A">SM11.GMD</rom>
			<rom type="file" offset="0x1B">SM12.GMD</rom>
			<rom type="shell" offset="0">NTMD</rom>
			<rom type="shell" offset="0">NTMDP</rom>
		</romlist>
		<titlelist>
			<title code="0x10">淫欲の乱れ</title>
			<title code="0x11">快楽への糸口</title>
			<title code="0x12">出口のない迷路</title>
			<title code="0x13">囚われの子猫達</title>
