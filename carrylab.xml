		<titlelist>
			<title code="0x01">TITLE MUSIC</title>
			<title code="0x02">SPRING MUSIC</title>
			<title code="0x03">SUMMER MUSIC</title>
			<title code="0x04">FALL MUSIC</title>
			<title code="0x05">WINTER MUSIC</title>
			<title code="0xff">’Ä—Ž‰¹</title>
		</titlelist>
	</game>
</gamelist>
